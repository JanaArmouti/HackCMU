{
    "name": "Neville Longbottom",
    "description": "Extension to encourage reading articles!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
    "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "storage"
    ],
    "options_ui": {
        "page": "options.html"
        "open_in_tab": false
    }
 }
